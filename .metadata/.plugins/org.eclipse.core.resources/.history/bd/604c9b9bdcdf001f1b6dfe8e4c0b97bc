package premierprojet;

public class Variables {

	public static void main(String[] args) {
		String name= "Mahamadou";
		System.out.println(name);
        
		int x = 15;
		int y = 24;
		System.out.println(x > y);
		
		float z= 0.1f;
	
		
		String studentName = "Mahamadou";
		int studentAge = 18;
		

		
		System.out.println("Nom de l'eleve: " + studentName);
		System.out.println("Age de l'eleve: " + studentAge);
		
		float a= 19.99f;
		System.out.println(a);
		
		char myVar1 = 65, myVar2 = 66, myVar3 = 67;
		System.out.println(myVar1);
		System.out.println(myVar2);
		System.out.println(myVar3);
		
		int age = 45;
		int voteage = 18;
		if (age>=voteage){
			System.out.print("assez ager");
		} else {
			System.out.print("pas assez ager");
			
		}
		
		
		

		
		
	}

}
